{
	"name": "store-api",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/store-api/src",
	"projectType": "application",
	"targets": {
		"buildCode": {
			"executor": "@nx/webpack:webpack",
			"outputs": [
				"{options.outputPath}"
			],
			"defaultConfiguration": "production",
			"options": {
				"target": "node",
				"compiler": "tsc",
				"outputPath": "dist/apps/store-api",
				"main": "apps/store-api/src/main.ts",
				"tsConfig": "apps/store-api/tsconfig.app.json",
				"generatePackageJson": true,
				"assets": [
					"apps/store-api/src/products/data/images"
				],
				"isolatedConfig": true,
				"webpackConfig": "apps/store-api/webpack.config.js"
			},
			"configurations": {
				"development": {},
				"production": {}
			}
		},
		"openApi": {
			"executor": "nx:run-commands",
			"dependsOn": [
				"buildCode"
			],
			"outputs": [
				"{workspaceRoot}/dist/apps/store-api/store-api-openapi.json"
			],
			"options": {
				"parallel": false,
				"cwd": "dist/apps/store-api",
				"commands": [
					"node ./main.js openApi"
				]
			}
		},
		"build": {
			"executor": "nx:run-commands",
			"dependsOn": [
				"buildCode",
				"openApi"
			],
			"outputs": [
				"{workspaceRoot}/dist/apps/store-api"
			],
			"options": {
				"parallel": false,
				"cwd": "dist/apps/store-api",
				"commands": [
					"npm i"
				]
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"dependsOn": [
				"^genClient"
			],
			"options": {
				"buildTarget": "store-api:buildCode",
				"inspect": true,
				"port": 9229
			},
			"configurations": {
				"development": {
					"buildTarget": "store-api:buildCode:development"
				},
				"production": {
					"buildTarget": "store-api:buildCode:production"
				},
				"openApi": {
					"buildTarget": "store-api:buildCode:development",
					"args": [
						"openApi"
					]
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"apps/store-api/**/*.ts"
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": [
				"{workspaceRoot}/coverage/{projectRoot}"
			],
			"options": {
				"jestConfig": "apps/store-api/jest.config.ts"
			}
		}
	},
	"tags": []
}